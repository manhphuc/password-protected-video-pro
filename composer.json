{
  "name": "yivic/yivic-ppvp",
  "type": "wordpress-plugin",
  "license": "GPL-2.0-or-later",
  "description": "Secure your videos with Password Protected Video Pro. Makes it easy for you to secure your video with a password set and requires viewers to do something to get the password to unlock your video.",
  "homepage": "https://yivic.net/",
  "authors": [
    {
      "name": "Phuc Nguyen",
      "role": "Developer",
      "email": "manhphucofficial@yahoo.com",
      "homepage": "https://github.com/manhphuc"
    }
  ],
  "keywords": [
    "protected-video",
    "wordpress-protected"
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.2",
    "guzzlehttp/guzzle": "^7.0",
    "illuminate/container": "^7.19"
  },
  "require-dev": {
    "symfony/var-dumper": "^5.1"
  },
  "autoload": {
    "psr-4": {
      "Yivic\\Wp\\Plugin\\": "src"
    }
  }
}
